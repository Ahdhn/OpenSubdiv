# set terminal
set term epslatex solid color

# set line styles
set style line 1 lw 4 lt rgb "dark-red"
set style line 2 lw 4 lt rgb "orange"
set style line 3 lw 4 lt rgb "dark-green"
set style line 4 lw 4 lt rgb "dark-blue"
set style line 5 lw 4 lt rgb "dark-orange"
set style line 6 lw 4 lt rgb "dark-violet"
set style line 7 lw 4 lt rgb "dark-goldenrod"
set style line 8 lw 4 lt rgb "black"

# key font size
set key font ",2"
